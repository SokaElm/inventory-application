<!DOCTYPE html>

<html>
  <head>
    <link rel="stylesheet" href="/styles.css" />
  </head>

  <body>
    <%- include('navbar', {links: links}) %>
    <h1>All categories</h1>
    <ul class="categoryCards">
      <% for (let i = 0; i < categories.length; i++) { %>
      <li class="category">
        <a href="<%= `${links[2].href}/${categories[i].category}`%>">
          <%=categories[i].category%></a
        >
      </li>
      <% }; %>
    </ul>
  </body>
</html>
